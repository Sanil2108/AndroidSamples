<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sanilk.com.userinterfaceandnavigation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdapterView" />
        <activity android:name=".ConstraintLayout" />
        <activity android:name=".RecyclerView" />
        <activity android:name=".GridView" />
        <activity android:name=".Notifications" />
        <activity android:name=".StatusBarActivity" />
        <activity android:name=".ViewPagerActivity" />
        <activity android:name=".NavigationBarActivity"></activity>
    </application>

</manifest>